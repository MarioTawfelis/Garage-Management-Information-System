<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="anchorPane" prefHeight="140.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="customers.gui.DeleteConfirmationController">
   <children>
      <VBox layoutX="35.0" layoutY="-64.0" prefHeight="140.0" prefWidth="340.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="340.0">
               <children>
                  <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets left="35.0" />
                     </HBox.margin>
                     <image>
                        <Image url="@question-icon.png" />
                     </image>
                  </ImageView>
                  <Label prefWidth="250.0" text="Are you sure you want to delete this customer?">
                     <HBox.margin>
                        <Insets left="15.0" right="15.0" top="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="35.0" top="30.0" />
               </padding>
            </HBox>
            <HBox prefWidth="400.0">
               <children>
                  <Button fx:id="yesButton" mnemonicParsing="false" onAction="#yesButtonClicked" prefHeight="25.0" prefWidth="100.0" text="Yes">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="noButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#noButtonClicked" prefHeight="25.0" prefWidth="100.0" text="No">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets left="160.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
